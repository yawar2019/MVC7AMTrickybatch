@model IPagedList<EmployeeModel>
@{
    ViewBag.Title = "Index";
}
@using CodeFirstApproach.Models
@using PagedList
@using PagedList.Mvc
<h2>Index</h2>

<table class="table table-bordered table-striped">
    <tr>
        <th>#Id</th>
        <th>Name</th>
        <th>Salary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>
@Html.PagedListPager(Model,i=>Url.Action("Index","Home",new {i}))

