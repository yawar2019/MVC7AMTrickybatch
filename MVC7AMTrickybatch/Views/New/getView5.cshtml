
@{
    ViewBag.Title = "getView5";
}

<h2>getView5</h2>


<table class="table table-bordered table-condensed table-striped">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>Salary</th>
    </tr>
    @foreach (var item in ViewBag.info)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>