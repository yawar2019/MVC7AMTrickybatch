@model List<MVC7AMTrickybatch.Models.EmployeeModel>
@{
    ViewBag.Title = "getView7";
}

<h2>getView7</h2>

<table class="table table-bordered table-responsive table-striped">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
        </tr>
    }
</table>
